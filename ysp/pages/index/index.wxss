.nav {
  position: fixed;
  top: 0;
  width: 100%;
  z-index: 1000;
}

.nav .navbar {
  position: fixed;
  width: 100%;
  top: 0rem;
  padding: 0rem .5rem;
  background-color: #323232;
}

.nav .search {
  position: relative;
  z-index: 99;
  padding: .625rem 3.5rem .625rem 0;
}

.nav .search .close {
  position: absolute;
  right: 1rem;
  top: 0;
  text-align: center;
  line-height: 3.125rem;
  color: #444;
}

.nav .search .input {
  display: block;
  padding: 0 0rem 0 10px;
  height: 1.875rem;
  line-height: 1.875rem;
  border: .0625rem solid #eee;
  border-radius: 1.875rem;
  background-color: #f7f7f7 !important;
  color: #333 !important;
}

.nav .search .submit {
  position: relative;
  width: 30px;
  height: 25px;
  line-height: 25px;
  top: -1.8rem;
  background-color: transparent;
  color: #666;
  border: 0;
  /* padding: .3125rem .625rem; */
  border-radius: 100%;
  text-align: center;
  cursor: pointer;
  z-index: 9999999;
  float: right;
  background-color: #f7f7f7 !important;
  color: #333 !important;
}

.nav .search-btns {
  position: absolute;
  top: .0625rem;
  right: 0;
  bottom: .0625rem;
  width: 2.8rem;
  height: 2.375rem;
  line-height: 2.375rem;
  z-index: 1;
  box-shadow: -.3125rem 0 .3125rem -.3125rem rgba(0, 0, 0, .1);
}

.nav .search-panel {
  position: relative;
  z-index: 99;
  top: 3rem;
}

.nav .search-panel .title {
  font-size: .875rem !important;
  position: relative;
  padding: 0 .9375rem !important;
  line-height: 2.5rem;
  overflow: hidden;

}

.nav .search-panel view.list {
  position: relative;
  line-height: 2.1875rem;
  width: 50%;
  float: left;
}

.nav .search-panel view.list>view {
  display: block;
  padding: 0 .9375rem;
  font-size: .875rem;
  height: 2.25rem;
  line-height: 2.25rem;

}

.nav .search-panel view.list>view .nums {
  position: relative;
  top: -.0625rem;
  font-size: .75rem;
  padding: .0625rem .3125rem;
  border-radius: .125rem;
  margin-right: .3125rem;
  color: #fff;
  background-color: #cacaca;
}

.num1 {
  background-color: #ff0d29 !important
}

.num2 {
  background-color: #ff920b !important
}

.num3 {
  background-color: #ffc600 !important
}

.nav .navbar>view {
  white-space: nowrap;
  overflow-y: hidden;
  overflow-x: auto;
  -webkit-overflow-scrolling: touch;
}

.nav .navbar view.menu {
  position: relative;
  margin-right: .9375rem;
  display: inline-block;
  padding: 0;
  height: 2.5rem;
  line-height: 2.5rem;
}

.nav .navbar view.main-this text {
  color: #FFE28A;
  font-weight: 700;

  display: block;
  font-size: .875rem;
}


.nav .navbar view.main-this text:before {
  content: "";
  position: absolute;
  left: 50%;
  bottom: 0;
  width: 100%;
  height: .25rem;
  border-radius: 3.125rem;
  background-color: #FFE28A;
  -webkit-transform: translate(-50%, 0);
  transform: translate(-50%, 0);
  z-index: 10;
}

